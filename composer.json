{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "aoxiang/province-city-area": "^1.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "barryvdh/laravel-snappy": "^0.4.7",
        "encore/redis-manager": "^0.1.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/psr7": "^1.6",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "hhxsv5/laravel-s": "~3.4.0",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^3.1",
        "laravel/telescope": "^1.2",
        "laravel/tinker": "^1.0",
        "overtrue/laravel-lang": "^3.0",
        "php-http/curl-client": "^1.7.1",
        "predis/predis": "^1.1",
        "qoraiche/laravel-mail-editor": "^2.0",
        "rinvex/countries": "^6.1",
        "sentry/sentry-laravel": "1.0.0",
        "spatie/laravel-medialibrary": "^7.0.0",
        "studio/laravel-totem": "^4.0.0",
        "tymon/jwt-auth": "^1.0",
        "wi1dcard/china-divisions": "^0.2.1",
        "yansongda/pay": "^2.6"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "lubusin/laravel-decomposer": "^1.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "wnx/laravel-stats": "^1.8.0",
        "zhuzhichao/ip-location-zh": "^2.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "hooks": {
            "pre-commit": [
                "echo committing as $(git config user.name)",
                "./vendor/phpunit/phpunit/phpunit"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
          "bootstrap/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cghooks": "vendor/bin/cghooks",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
